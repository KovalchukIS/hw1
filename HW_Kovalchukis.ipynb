    "from typing import List\n",
    "\n",
    "import databases\n",
    "import sqlalchemy as sa \n",
    "from fastapi import FastAPI, Depends\n",
    "from pydantic import BaseModel\n",
    "\n",
    "import datetime\n",
    "import random\n",
    "from dateutil.relativedelta import relativedelta\n",
    "#from aiopg.sa import create_engine\n",
    "\n",
    "from sqlalchemy.ext.asyncio import AsyncSession, create_async_engine\n",
    "from sqlalchemy.ext.declarative import declarative_base\n",
    "from sqlalchemy.orm import sessionmaker\n",
    "\n",
    "\n",
    "DB_USER = \"postgres\"\n",
    "DB_NAME = \"study\"\n",
    "DB_PASSWORD = \"postgres\"\n",
    "DB_HOST = \"127.0.0.1\"\n",
    "\n",

    "\n",
    "DATABASE_URL = f\"postgresql://{DB_USER}:{DB_PASSWORD}@{DB_HOST}:5432/{DB_NAME}\"\n",
    "database = databases.Database(DATABASE_URL)\n",
    "metadata = sa.MetaData()\n",
    "\n",
    "\n",
    "stores = sa.Table(\n",
    "    \"tt.store\",\n",
    "    metadata,\n",
    "    sa.Column(\"id\", sa.Integer, primary_key=True),\n",
    "    sa.Column(\"address\", sa.String),\n",
    ")\n",
    "\n",
    "items = sa.Table(\n",
    "    \"tt.item\",\n",
    "    metadata,\n",
    "    sa.Column(\"id\", sa.Integer, primary_key=True),\n",
    "    sa.Column(\"name\", sa.String, unique=True),\n",
    "    sa.Column(\"price\", sa.Float),\n",
    ")\n",
    "\n",
    "sales = sa.Table(\n",
    "    \"tt.sale\",\n",
    "    metadata,\n",
    "    sa.Column(\"id\", sa.Integer, primary_key=True),\n",
    "    sa.Column(\"sale_time\", sa.DateTime, nullable=False, default=datetime.datetime.now()),\n",
    "    sa.Column(\"item_id\", sa.Integer, sa.ForeignKey('tt.item.id')),\n",
    "    sa.Column(\"store_id\", sa.Integer, sa.ForeignKey('tt.store.id')),\n",
    ")\n",
    "\n",
    "\n",
    "engine = sa.create_engine(DATABASE_URL)\n",
    "metadata.create_all(engine)\n",
    "\n",
    "class Store(BaseModel):\n",
    "    id: int\n",
    "    address: str\n",
    "\n",
    "class StoreIn(BaseModel):\n",
    "    address: str\n",
    "\n",
    "\n",
    "class Item(BaseModel):\n",
    "    id: int\n",
    "    name: str\n",
    "    prise: float\n",
    "\n",
    "class ItemIn(BaseModel):\n",
    "    name: str\n",
    "    prise: float\n",
    "\n",
    "\n",
    "class Sale(BaseModel):\n",
    "    id: int\n",
    "    sale_time: datetime.datetime\n",
    "    item_id: int\n",
    "    store_id: int\n",
    "\n",
    "class SaleIn(BaseModel):\n",
    "    sale_time: datetime.datetime\n",
    "    item_id: int\n",
    "    store_id: int\n",
    "\n",
    "class SaleInProd(BaseModel):\n",
    "    item_id: int\n",
    "    store_id: int\n",
    "\n",
    "class StoreTop(BaseModel):\n",
    "    id: int\n",
    "    address: str\n",
    "    income: float\n",
    "\n",
    "class ItemsTop(BaseModel):\n",
    "    id: int\n",
    "    name: str\n",
    "    sales_amount: int\n",
    "\n",
    "await database.connect()\n",
    "for i in range(1,51):\n",
    "    query = stores.insert().values(address=f\"Новый город, улица Первая, дом {i}\")\n",
    "    last_record_id = await database.execute(query)\n",
    "    #print ({\"id\": last_record_id})\n",
    "\n",
    "for i in range(1,51):\n",
    "    query = items.insert().values(name=f'Товар_{i}',price=round(random.uniform(1000.0, 100000.0),2))\n",
    "    last_record_id = await database.execute(query)\n",
    "    #print ({\"id\": last_record_id})\n",
    "\n",
    "start_date = (datetime.date.today().replace(day=1)- relativedelta(months=2)).toordinal()\n",
    "end_date = datetime.date.today().toordinal()\n",
    "\n",
    "for i in range(1,2001):\n",
    "    query = sales.insert().values(sale_time=datetime.date.fromordinal(random.randint(start_date, end_date)),\n",
    "                                  item_id=random.randint(1,50),\n",
    "                                  store_id=random.randint(1,50)   )\n",
    "    last_record_id = await database.execute(query)\n",
    "    #print ({\"id\": last_record_id})\n",
    "\n",
    "await database.disconnect()\n",
    "\n",
    "app = FastAPI()\n",
    "\n",
    "DATABASE_ASYNC_URL = f\"postgresql+asyncpg://{DB_USER}:{DB_PASSWORD}@{DB_HOST}:5432/{DB_NAME}\"\n",
    "engine_as = create_async_engine(DATABASE_ASYNC_URL)  \n",
    "Base = declarative_base() \n",
    "async_session_maker = sessionmaker(engine_as, class_=AsyncSession, expire_on_commit=False)\n",
    "\n",
    "@app.get(\"/stores/\", response_model=List[Store])\n",
    "async def read_stores():\n",
    "    async with async_session_maker() as session:\n",
    "        query = stores.select()\n",
    "        result =    await session.execute(query)\n",
    "    return result.all()\n",
    "\n",
    "@app.get(\"/items/\", response_model=List[Item])\n",
    "async def read_items():\n",
    "    async with async_session_maker() as session:\n",
    "        query = items.select()\n",
    "        result =    await session.execute(query)\n",
    "    return result.all()\n",
    "    \n",
    "@app.get(\"/stores/top/\", response_model=List[StoreTop])\n",
    "async def top_stores():\n",
    "    async with async_session_maker() as session:\n",
    "        query = f\"\"\"\n",
    "        select store.id as id,\n",
    "            store.address as address, \n",
    "            round(sum(item.price)::numeric,2) as income\n",
    "        from public.\"tt.sale\" sale\n",
    "    \tleft join public.\"tt.item\" item on item.id =sale.item_id \n",
    "    \tleft join public.\"tt.store\" store on store.id =sale.store_id \n",
    "    \twhere sale.sale_time>=date'{(datetime.datetime.now()- relativedelta(months=1)).strftime(\"%Y-%m-%d\")}'\n",
    "        group by 1,2\n",
    "        order by 3 desc limit 10\n",
    "        \"\"\"\n",
    "        result =    await session.execute(query)\n",
    "        \n",
    "    return result.all()\n",
    "    \n",
    "\n",
    "@app.get(\"/items/top/\", response_model=List[ItemsTop])\n",
    "async def top_items():\n",
    "    async with async_session_maker() as session:\n",
    "        query = f\"\"\"\n",
    "                select item.id,\n",
    "            item.name, \n",
    "            count(sale.id) as sales_amount\n",
    "        from public.\"tt.sale\" sale\n",
    "    \tleft join public.\"tt.item\" item on item.id =sale.item_id \n",
    "    \tgroup by 1,2\n",
    "        order by 3 desc limit 10\n",
    "        \"\"\"\n",
    "        result =    await session.execute(query)\n",
    "        \n",
    "    return result.all()\n",
    "\n",
    "\n",
    "\n",
    "@app.post(\"/sales/\")#, response_model=SaleInProd)\n",
    "async def add_sales(new_sales: SaleInProd):\n",
    "    async with async_session_maker() as session:\n",
    "        print(new_sales)\n",
    "        query = sales.insert().values(**new_sales)\n",
    "        last_record_id = await session.execute(query)\n",
    "        await session.commit()\n",
    "        return {**new_sales, \"id\": last_record_id}\n",
    "    "
   ]
